    "# write usld json to csv\n",
    "def _write_to_csv(csv_file, files):\n",
    "    from collections import defaultdict\n",
    "    headers = ['_id', 'fullName', 'brandName', 'langualCode', \n",
    "               'langualCodeDescription', 'allIngredients', \n",
    "               'netContents','claims', 'offMarket']\n",
    "    f = files\n",
    "    with open(f, 'r', encoding = 'utf-8') as file:\n",
    "                f = json.load(file)\n",
    "\n",
    "    item = defaultdict(list)\n",
    "    def extract_values(r):\n",
    "        if isinstance(r, dict):\n",
    "            for key in r.keys():\n",
    "                if key in headers:\n",
    "                    item[key].extend([r[key]])\n",
    "                extract_values(r[key])\n",
    "\n",
    "    with open(csv_file, mode='a', newline='', encoding='utf-8') as file:\n",
    "        writer = csv.writer(file)\n",
    "        if not os.path.exists(csv_file) or os.stat(csv_file).st_size == 0:\n",
    "            writer.writerow(headers)\n",
    "        \n",
    "        for i in range(len(f['hits'])):\n",
    "            item = defaultdict(list)\n",
    "            extract_values(f['hits'][i])\n",
    "            writer.writerow([item[h] for h in headers])\n",
    "    \n",
    "    print(f'Data has been successfully written to {csv_file}')\n",
    "\n",
    "# _write_to_csv('california_poppy_usld.csv', 'california_poppy.json')"
   ]
